{
    "manifest_version": 2,
    "name": "CookBook",
    "version": "0.1",
    "author": "Shelby Joyner",
    "description": "Your personal cookbook assistant",
    "permissions": [
      ""
    ],
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": [
          "content.js"
        ]
      }
    ],
    "background": {
      "scripts": [
        "background.js"
      ]
    },
    "browser_action": {
      "default_popup": "frontend/public/index.html",
      "default_title": "Your personal cookbook assistant"
    }
  }